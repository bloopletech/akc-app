<?xml version='1.0' encoding='utf-8'?>
<widget id="link.akc.app" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ArrowKeyControl</name>
    <description>
    A fun game.
  </description>
    <author email="i@bloople.net" href="https://i.bloople.net">
    Brenton Fletcher
  </author>
    <content src="https://akc.link/" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-navigation href="*://*.akc.link/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <splash src="res/ios/Default@2x~universal~anyany.png" />
    </platform>
    <plugin name="cordova-plugin-android-window-background" spec="^1.0.2">
        <variable name="WINDOW_BACKGROUND_COLOR" value="#f1efc0" />
    </plugin>
    <engine name="android" spec="^7.0.0" />
    <engine name="ios" spec="4.5.5" />
</widget>
